<%- include("partials/auth-header.ejs") %>
<section id="register">
     <form action="/register" id="userInfo" method="post">
        
        <% if(errors == true){%>
          <div class="container">
            <form class="register-row">
              <div class="register-column">
                <label for="validationServer01" class="form-label">First Name:</label>
                <% if(registrationErrors.firstName != null){%>   
                  <input type="text" class="form-control is-invalid fname" id="validationServer01" name="firstName" aria-describedby="inputGroupPrepend3 validationServerUsernameFeedback" required>
                  <div id="validationServer01Feedback" class="invalid-feedback">
                   <p> <%= registrationErrors.firstName %></p>
                  </div>
                  <%}%>

                  <% if(registrationErrors.firstName == null){%>   
                    
                    <input type="text" class="form-control fname" name="firstName" id="validationServer01" value=<%= registrationValues.firstName %> required>
          
                    <%}%>
                
              </div>

              <div class="register-column">
                <label for="validationServer01" class="form-label">Last Name:</label>
                <% if(registrationErrors.lastName != null){%>   
                  <input type="text" class="form-control is-invalid lname" id="validationServer01" name="lastName" aria-describedby="inputGroupPrepend3 validationServerUsernameFeedback" required>
                  <div id="validationServer01Feedback" class="invalid-feedback">
                   <p> <%= registrationErrors.lastName %></p>
                  </div>
                  <%}%>

                  <% if(registrationErrors.lastName == null){%>   
                    
                    <input type="text" class="form-control lname" name="lastName" id="validationServer01" value=<%= registrationValues.lastName %> required>
          
                    <%}%>
                
              </div>
              
              <div class="register-column">
                <label for="validationEmail" class="form-label">E-mail:</label>
                <% if(registrationErrors.email != null){%>   
                  <input type="text" class="form-control is-invalid" id="validationServerEmail" name="email" aria-describedby="inputGroupPrepend3 validationServerEmailFeedback" required>
                  <div id="validationServerEmailFeedback" class="invalid-feedback">
                    <%= registrationErrors.email %>
                  </div>
                  <%}%>

                  <% if(registrationErrors.email == null){%>   
                    
                    <input type="text" class="form-control" name="email" value=<%= registrationValues.email %> id="validationServerEmail" required>
                
                    <%}%>
                
              </div>
              <div class="register-column">
                <label for="validation04" class="form-label">Phone Number:</label>
                <% if(registrationErrors.contact != null){%>
                  <span class="input-group-text" id="inputGroupPrepend2">+91</span>
                  <input type="text" class="form-control is-invalid" name="contact" id="validationServer04" aria-describedby="inputGroupPrepend3 validationServer04Feedback" required>
                  <div id="validationServer04Feedback" class="invalid-feedback">
                    <%= registrationErrors.contact %>
                  </div>
                  <%}%>

                  <% if(registrationErrors.contact == null){%>
                  
                  <input type="text" class="form-control" name="contact" value=<%= registrationValues.contact %> id="validationServer04" required>
               
                    <%}%>
                
              </div>

              <div class="register-column">
                <label for="validationPassword" class="form-label">Password:</label>
                <% if(registrationErrors.password != null){%>   
                  <input type="password" class="form-control is-invalid" name="password" id="validationServerPassword" aria-describedby="inputGroupPrepend3 validationServerPasswordFeedback" required>
                  <div id="validationServerPasswordFeedback" class="invalid-feedback">
                    <%= registrationErrors.password %>
                  </div>
                  <%}%>

                  <% if(registrationErrors.Password == null & registrationErrors.confirmPassword != null ){%>
                    
                    <input type="password" class="form-control " name="password" id="validationServerPassword" required>
                
                    <%}%>
                    <% if(registrationErrors.email != null){%>
                    
                      <input type="password" class="form-control " name="password" id="validationServerPassword" required>
                  
                      <%}%>
                
              </div>

              <div class="register-column">
                <label for="validationPassword" class="form-label">Confirm Password:</label>
                <% if(registrationErrors.confirmPassword != null){%>
                  <input type="password" class="form-control is-invalid" name="confirmPassword" id="validationServerPassword" aria-describedby="inputGroupPrepend3 validationServerPasswordFeedback" required>
                  <div id="validationServerPasswordFeedback" class="invalid-feedback">
                    <%= registrationErrors.confirmPassword %>
                  </div>
                  <%}%>

                  <% if(registrationErrors.confirmPassword == null){%>
                    
                    <input type="password" class="form-control " name="confirmPassword" id="validationServerPassword"required>
                
                    <%}%>
                
              </div>
              
              <div class="register-column">
                <label for="validation03" class="form-label">Address:</label>
                <% if(registrationErrors.address != null){%>
                  <p></p>
                  <input type="text" class="form-control is-invalid" name="address" id="validationServer03"  aria-describedby="inputGroupPrepend3 validationServer03Feedback" required>
                  <div id="validationServer03Feedback" class="invalid-feedback">
                    <%= registrationErrors.address %>
                  </div>
                  <%}%>

                  <% if(registrationErrors.address == null){%>
                    
                    <input type="text" class="form-control" name="address" id="validationServer03" value=<%= registrationValues.address %> required>
                
                    <%}%>
                
              </div>

              <div class="register-column">
                <label for="validation05" class="form-label">Zip:</label>
                <% if(registrationErrors.pinCode != null){%>
                  <p></p>
                  <input type="text" class="form-control is-invalid" name="pinCode" id="validationServer05" aria-describedby="inputGroupPrepend3 validationServer05Feedback" required>
                  <div id="validationServer05Feedback" class="invalid-feedback">
                    <%= registrationErrors.pinCode %>
                  </div>
                  <%}%>

                  <% if(registrationErrors.pinCode == null){%>
                    
                  <input type="text" class="form-control" name="pinCode" id="validationServer05" value=<%= registrationValues.pinCode %> required>
               
                    <%}%>
                
              </div>
              <div class="register-column">
                <button class="btn btn-primary" type="Register">Register</button>
              </div>
            </form>
          </div>
        <%}%>

       
        
        <% if(errors != true) {%>
          <div class="container">
            <div class="register-row">
              <div class="register-column fname">
                <label for="validationDefault01" class="form-label">First Name:</label>
                <input type="text" class="form-control" name="firstName" id="validationDefault01" required>
              </div>

              <div class="register-column lname">
                <label for="validationDefault01" class="form-label">Last Name:</label>
                <input type="text" class="form-control" name="lastName" id="validationDefault01" required>
              </div>
              
              <div class="register-column">
                <label for="validationDefaultEmail" class="form-label">E-mail:</label>
                <div class="input-group">
                  <input type="email" class="form-control" name="email" id="validationDefaultEmail"  aria-describedby="inputGroupPrepend2" required>
                </div>
              </div>

              <div class="register-column">
                <label for="validationDefault04" class="form-label">Phone Number:</label>
                <div class="input-group">
                  <span class="input-group-text" id="inputGroupPrepend2">+91</span>
                  <input type="text" class="form-control" name="contact" id="validationDefault04"  aria-describedby="inputGroupPrepend2" required>
                </div>
              </div>

              <div class="register-column">
                <label for="validationDefaultPassword" class="form-label">Password:</label>
                <input type="password" class="form-control" name="password" id="validationDefaultPassword" required>
              </div>
              <div class="register-column">
                <label for="validationDefaultPassword" class="form-label">Confirm Password:</label>
                <input type="password" class="form-control" name="confirmPassword" id="validationDefaultPassword" required>
              </div>
              <div class="register-column">
                <label for="validationDefault03" class="form-label">Address:</label>
                <input type="text" class="form-control" name="address" id="validationDefault03" required>
              </div>
              <div class="register-column">
                <label for="validationDefault05" class="form-label">Zip:</label>
                <input type="text" class="form-control" name="pinCode" id="validationDefault05" required>
              </div>
              
              <div class="register-column">
                <button class="btn btn-primary" type="submit" style="width: 100%; margin-top:1rem">Register</button>
              </div>
            </div>
          </div>
        <%}%>
      </form>
      </section>
      <%- include("partials/footer.ejs") %>